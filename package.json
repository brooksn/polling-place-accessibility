{
  "name": "polling-place-accessibility-app",
  "version": "1.0.0",
  "description": "Application to improve availability of information about polling place features that affect accessibility for voters.",
  "scripts": {
    "clean": "rimraf dist",
    "copy": "cp -r public/* dist && cp index.html dist",
    "prebuild": "npm run clean",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:sass": "gulp sass",
    "build:bower": "cp bower.json dist && cd dist && bower install",
    "build": "npm run build:webpack && npm run build:sass && npm run build:bower",
    "postbuild": "npm run copy",
    "start:bower": "bower install",
    "start:gulp": "gulp sass",
    "start": "npm run start:bower && npm run start:gulp && node devServer.js",
    "watch": "gulp",
    "test": "tape -r babel-register tests/**/*.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abustamam/polling-place-accessibility.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "webpack",
    "babel",
    "react-transform",
    "voting",
    "polling",
    "accessibility",
    "Hack4Sac",
    "CodeForSacramento"
  ],
  "author": "Hack4Sac Voting Accessibility team (http://github.com/abustamam/polling-place-accessibility)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/abustamam/polling-place-accessibility/issues"
  },
  "homepage": "https://github.com/abustamam/polling-place-accessibility",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-register": "^6.7.2",
    "cross-env": "^1.0.6",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-load-plugins": "^1.1.0",
    "gulp-sass": "^2.1.0",
    "image-webpack-loader": "^1.6.3",
    "node-sass": "^3.4.2",
    "rimraf": "^2.4.3",
    "tape": "^4.5.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "babel-core": "^6.6.5",
    "join-component": "^1.1.0",
    "jquery": "^2.2.3",
    "leaflet": "^0.7.7",
    "lodash": "^4.8.2",
    "lodash.has": "^4.3.0",
    "lodash.set": "^4.1.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-leaflet": "^0.10.1",
    "superagent": "^1.8.0-beta.2"
  }
}
